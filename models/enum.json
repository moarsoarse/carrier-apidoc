{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Список",
  "type": "object",
  "description": "Схема перечислений",
  "required": [
    "ID",
    "title"
  ],
  "properties": {
    "ID": {
      "type": "string",
      "format": "uuid"
    },
    "title": {
      "type": "string",
      "description": "Наименование"
    },
    "desc": {
      "type": "string",
      "description": "Описание"
    }
  }
}