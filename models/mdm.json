{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Запись справочника",
  "x-tags": [
    "common"
  ],
  "type": "object",
  "description": "Общие атрибуты элементов справочников",
  "properties": {
    "ID": {
      "type": "string",
      "description": "Идентификатор записи",
      "format": "uuid"
    },
    "created": {
      "type": "string",
      "format": "date-time",
      "description": "Создана"
    },
    "creator": {
      "$ref": "user.json",
      "description": "Создал"
    },
    "modified": {
      "type": "string",
      "format": "date-time",
      "description": "Изменена"
    },
    "modifier": {
      "$ref": "user.json",
      "description": "Изменил"
    }
  }
}